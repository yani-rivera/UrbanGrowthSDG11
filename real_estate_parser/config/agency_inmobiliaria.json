{
  "agency": "Inmobiliaria",
  "nemonic":"INMB",
  "preprocessing_needed": true,
  "header_marker": "#",
  "listing_marker": "*",  
  "listing_marker_tochange": "*",   
  "auto_masquerade_numdot": false,
  "sanitize": false,
  "glue_price_tails": true,
  "glue_area_tails": true,
  "preprocess": [1,2],
  "emit_marker": false,
  "price_autofix_locale": "auto",


  "symbols": ["*", "•", ">", "-"],
  "start_exceptions": [
    "Mts.", "Mts", "mt2", "m²", "mts", "mts2", "Mts. ",
    "Lps", "Lps.", "L.", "$", "Lps. ",
    "vr2", "vrs", "Vrs", "Vr²", "Vrs²",
    "Res.", "res.", "area", "área",
    "baños", "baño", "habitaciones", "hab.", "cubículos", "dormitorios",
    "garaje", "lavandería", "cocina", "sala", "comedor"
  ],

  "neighborhood_rule": {
    "strategy": "UPPERCASE", 
    "strategy_order": ["delimiter"],
    "prefix_tokens": ["col.", "colonia", "res.", "resid.", "residencial", "urb.", "bo.", "barrio", "ave.", "av."],    
    "max_token_span": 35
  },

  "neighborhood_flow": {"flow_type": "simple"},

  "bathroom_keywords": ["baño", "baños", "banos", "bafios", "ba√±o", "ba√±os","bañod"],
  "bathroom_infer_from_bedrooms": true,
  "bathroom_ensuite_regex": true,
  "bathroom_ensuite_markers": [
  "cada\\s+un[oa].{0,40}bañ" ,
  "cada una con su baño",
  "cada uno con su baño",
  "cada habitación con su baño",
  "cada recamara con su baño",
  "cada recámara con su baño",
  "cada cuarto con su baño",
  "todas con baño",
  "todas con bano",
  "con su baño"
   ],


  "currency_aliases": { "L.": "HNL", "$": "USD", "US$": "USD" ,"$": "USD","Lps,":"HNL","Lps, ":"HNL","US$ ": "USD","L":"HNL","L ":"HNL","Lps.":"HNL","Lps. ":"HNL","Lps":"HNL"},
  
  "area_aliases": {
    "ac": ["m²","m2","mt2","mts2","metros cuadrados"],
    "at": ["vrs²","vrs2","vr2","varas cuadradas"],
    "mz": ["mz","manzana","manzanas"] 
  },
 "area_unit_conversions_m2": {
  "m²": 1, "m2": 1, "mt2": 1, "mts2": 1, "metros cuadrados": 1,
  "vrs²": 0.705012, "vrs2": 0.705012, "vr2": 0.705012, "varas cuadradas": 0.705012,
  "mz": 7050, "manzana": 7050, "manzanas": 7050
 },

 "area_preferences": {
  "prefer_near_price": true,
  "window_chars": 60,
  "min_m2": 10,
  "max_m2": 20000
},
 "area_routing": "by_unit",
"area_swap_guard": true,
"area_units": {
  "AC_m2": ["m2","m²","mt2","mts2","metro cuadrado","metros cuadrados"],
  "AT_v2": ["v2","v²","vara","varas","varas cuadradas","varas2","vrs2","vz2","vr2","manzana","manzanas","mz"]
},


  "type_keywords": {
    "Apartment": ["apto","apartamento"],
    "House": ["casa","res."],
    "Commercial": ["local","bodega"],
    "Land": ["terreno","lote"]
  },
"parsing_overrides": {
  "allow_slash_bed_bath": true,
  "price_accept_mil": true,
  "price_accept_k": true,
  "price_require_currency": true,
  "treat_price_per_m2_as": "skip",
  "price_keywords": [
    "precio","venta","alquiler","renta",
    "mensual","/mes","por mes"
  ],
  "multi_price_policy": "range",
  "multi_price_pair_by_area": true, 
  "multi_price_area_window_chars": 40,
  "multi_price_max_variants": 4
 },


  "section_headers": [
    {
      "pattern": "CASAS EN ALQUILER",
      "transaction": "Rent",
      "type": "House",
      "category": "CASAS EN ALQUILER"
    },
    {
      "pattern": "APARTAMENTOS EN ALQUILER CON MUEBLES Y SIN MUEBLES",
      "transaction": "Rent",
      "type": "Apartment",
      "category": "APARTAMENTOS EN ALQUILER CON MUEBLES Y SIN MUEBLES"
    }
  
  ]
}


