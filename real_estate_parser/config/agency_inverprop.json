{
  "agency": "Inverprop",
  "preprocessing_needed": true,
  "header_marker": "#",
  "listing_marker": "*",      
  "auto_masquerade_numdot": false,
  "sanitize": false,
  "glue_price_tails": true,
  "glue_area_tails": false,
  "preprocess": [2],
  "emit_marker": false,


  "neighborhood_rule": {
    "strategy": "first_comma", 
    "strategy_order": ["delimiter"],
    "prefix_tokens": ["col.", "colonia", "res.", "resid.", "residencial", "urb.", "bo.", "barrio", "ave.", "av."],    
    "max_token_span": 20
  },

  "neighborhood_flow": {
    "flow_type": "simple"},

  "bathroom_keywords": ["baño", "baños", "banos", "bafios", "ba√±o", "ba√±os","bañod"],
  "bathroom_infer_from_bedrooms": true,
  "bathroom_ensuite_regex": true,
  "bathroom_ensuite_markers": [
  "cada\\s+un[oa].{0,40}bañ" ,
  "cada una con su baño",
  "cada uno con su baño",
  "cada habitación con su baño",
  "cada recamara con su baño",
  "cada recámara con su baño",
  "cada cuarto con su baño",
  "todas con baño",
  "todas con bano",
  "con su baño"
   ],


  "currency_aliases": { "L.": "HNL", "$": "USD", "US$": "USD" ,"$": "USD","Lps,":"HNL","Lps, ":"HNL","US$ ": "USD","L":"HNL","L ":"HNL"},
  
  "area_aliases": {
    "ac": ["m²","m2","mt2","mts2","metros cuadrados"],
    "at": ["vrs²","vrs2","vr2","varas cuadradas"],
    "mz": ["mz","manzana","manzanas"]
  },
 "area_unit_conversions_m2": {
  "m²": 1, "m2": 1, "mt2": 1, "mts2": 1, "metros cuadrados": 1,
  "vrs²": 0.705012, "vrs2": 0.705012, "vr2": 0.705012, "varas cuadradas": 0.705012,
  "mz": 7050, "manzana": 7050, "manzanas": 7050
 },

 "area_preferences": {
  "prefer_near_price": true,
  "window_chars": 60,
  "min_m2": 10,
  "max_m2": 20000
},
 "area_routing": "by_unit",
"area_swap_guard": true,
"area_units": {
  "AC_m2": ["m2","m²","mt2","mts2","metro cuadrado","metros cuadrados"],
  "AT_v2": ["v2","v²","vara","varas","varas cuadradas","varas2","vrs2","vz2","vr2","manzana","manzanas","mz"]
},


  "type_keywords": {
    "Apartment": ["apto","apartamento"],
    "House": ["casa","res."],
    "Commercial": ["local","bodega"],
    "Land": ["terreno","lote"]
  },
"parsing_overrides": {
  "allow_slash_bed_bath": true,
  "price_accept_mil": true,
  "price_accept_k": true,
  "price_require_currency": true,
  "treat_price_per_m2_as": "skip",
  "price_keywords": [
    "precio","venta","alquiler","renta",
    "mensual","/mes","por mes"
  ],
  "multi_price_policy": "range",
  "multi_price_pair_by_area": true, 
  "multi_price_area_window_chars": 40,
  "multi_price_max_variants": 4
},


"section_headers": [
  { "pattern": "#ALQUILER DE CASAS",         "transaction": "Rent",  "type": "House",      "category": "ALQUILER DE CASAS" },
  { "pattern": "#ALQUILER DE APARTAMENTOS",  "transaction": "Rent",  "type": "Apartment",  "category": "ALQUILER DE APARTAMENTOS" },
  { "pattern": "#ALQUILER DE BODEGAS:",      "transaction": "Rent",  "type": "Commercial", "category": "ALQUILER DE BODEGAS" },
  { "pattern": "#ALQUILER DE LOCALES:",      "transaction": "Rent",  "type": "Commercial", "category": "ALQUILER DE LOCALES" },
  { "pattern": "#ALQUILER DE TERRENOS",      "transaction": "Rent",  "type": "Land",       "category": "ALQUILER DE TERRENOS" },
  { "pattern": "#PROPIEDADES COMERCIALES",   "transaction": "Sale",  "type": "Commercial", "category": "PROPIEDADES COMERCIALES" },
  { "pattern": "#VENTA DE CASAS",            "transaction": "Sale",  "type": "House",      "category": "VENTA DE CASAS" },
  { "pattern": "#VENTA DE TERRENOS",         "transaction": "Sale",  "type": "Land",       "category": "VENTA DE TERRENOS" }
]

}
