{"agency": "Inmovivienda",
  "nemonic":"INMVD",
  "preprocessing_needed": true,
  "listing_marker": "*",
  "auto_masquerade_numdot": false,
  "sanitize": true,
  "glue_price_tails": true,
  "glue_area_tails": true,
  "preprocess": [2],
  "emit_marker": false, 
   "neighborhood_rule": {
    "strategy": "before_colon", 
    "strategy_order": ["delimiter"],
    "prefix_tokens": ["col.", "colonia", "res.", "resid.", "residencial", "urb.", "bo.", "barrio", "ave.", "av."],    
    "max_token_span": 20
  },

    "neighborhood_flow": {"flow_type": "simple"},
  
"mode": "plain",
  "windows": {
  "head_window_chars": 60,
  "price_lookahead_lines": 2,
  "price_lookahead_chars": 320,
  "anchor_lookback_chars": 60,
  "min_anchor_gap": 10
},


"start_gate": {
  "block_price_first": true,
  "block_area_number_first": true,
  "block_feature_words_first": true,

  "family_tags": ["Res.", "Col.", "Colonia", "Barrio", "Bo."],
  "family_soft_validate": true,           
  "connectors": ["de","del","la","las","los","y","el"],
  "family_name_max_tokens": 7,

  "allow_numeric_date_neighborhoods": true,
  "numeric_date_pattern": "(?i)^[\\s\"'\u201c\u201d\u2018\u2019]*(\\d{1,2})(?:\u00ba|ro|er)?\\s+(?:de|del)\\s+(?:enero|febrero|marzo|abril|mayo|junio|julio|agosto|septiembre|setiembre|octubre|noviembre|diciembre)\\b",

  "qualifiers_after_name": [
    "zona\\s+\\d+","sector\\s+\\d+",
    "etapa\\s+(?:[IVXLC]+|\\d+)","fase\\s+\\d+",
    "km\\s+\\d+","anexo","manzana\\s+[A-Z0-9]+","bloque\\s+\\w+"
  ]
},

"gazetteer": {
  "enabled": true,
  "mode": "hint",                       
  "prefer_document_lexicon": true,     
  "city_gazetteer_path": "config/config_known_neighborhoods.json",
  "fuzzy_max_distance": 1,
  "max_influenced_starts": 30,
  "cooldown_lines": 2
},


"headers": { "keep": true, "preserve_whitespace": true },
"inline_split": { "clone_per_price": false },







  "currency_aliases": {
    "$": "USD",
    "$.": "USD",
    "$. ": "USD",
    "$ ": "USD",
    "L": "HNL",
    "L.": "HNL",
    "Lps": "HNL",
    "Lps.": "HNL",
    "L. ": "HNL",
    "LPS.": "HNL",
    "L ": "HNL"
  },
  "symbols": ["*", "•", ">", "-"],
  "start_exceptions": [
    "Mts.", "Mts", "mt2", "m²", "mts", "mts2", "Mts. ",
    "Lps", "Lps.", "L.", "$", "Lps. ",
    "vr2", "vrs", "Vrs", "Vr²", "Vrs²",
    "Res.", "res.", "area", "área",
    "baños", "baño", "habitaciones", "hab.", "cubículos", "dormitorios",
    "garaje", "lavandería", "cocina", "sala", "comedor"
  ],
  "neighborhood_aliases": {
    "COL. PALMIRA": ["PALMIRA", "COL PALMIRA", "COLONIA PALMIRA"],
    "COL. LOS ANGELES": ["LOS ANGELES", "COL LOS ANGELES"],
    "RES. MONSEÑOR FIALLOS": ["MONSEÑOR FIALLOS"]
  },

    "area_aliases": {
    "ac": ["m²", "m2", "mt2", "mts2", "mts²", "metros", "metros2", "metros²"],
    "at": ["vrs²", "vrs2", "vr2", "vr²", "vrs", "vara", "varas", "v²", "vr"],
    "mz": ["manzana", "manzanas", "mz"]
  },

  
  "range_separators": ["/", "-", " a ", " hasta ", "–", "—"],

  "parsing_overrides": {
    "bathrooms_require_keyword": true,
    "bathrooms_keywords": ["baño", "baños", "bano", "banos", "bath", "baths", "ba."],
    "bathrooms_block_near_tokens": ["L.", "Lps", "USD", "US$", "$"],
    "bathrooms_block_near_area_units": ["m2", "m²", "vrs2", "vrs²", "vr2", "vr²", "vara", "varas"],
    "bathrooms_max_value": 6,
    "bathrooms_window_tokens": 6,
    "allow_slash_bed_bath": true,
    "price_accept_mil": true,
    "price_accept_k": true,
    "price_require_currency": true,
    "treat_price_per_m2_as": "skip",
    "inherit_currency_in_ranges": true,
    "inherit_currency_in_ranges_min_value": 1000,
    "multi_price_policy": "first_only",
  
      "price_keywords": [
       "precio","venta","alquiler","renta",
       "mensual","/mes","por mes"
    ]
  },
  "section_headers": [
    { "pattern": "ALQUILER DE APARTAMENTOS", "transaction": "Rent", "type": "Apartment", "category": "ALQUILER DE APARTAMENTOS" },
    { "pattern": "ALQUILER DE CASAS", "transaction": "Rent", "type": "House", "category": "ALQUILER DE CASAS" },
    { "pattern": "VENTA DE APARTAMENTOS", "transaction": "Sale", "type": "Apartment", "category": "VENTA DE APARTAMENTOS" },
    { "pattern": "VENTA DE CASAS", "transaction": "Sale", "type": "House", "category": "VENTA DE CASAS" },
    { "pattern": "VENTA DE TERRENOS", "transaction": "Sale", "type": "Land", "category": "VENTA DE TERRENOS" }
  ]
}
